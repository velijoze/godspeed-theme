  {
    "name": "Background BreadCrumb",
    "settings": [
      {
        "type": "image_picker",
        "id": "bg_breadcrumb",
        "label": "Background Image BreadCrumb",
        "info": "1920x510px recommended"
      }
	]
  }
{% endschema %}

